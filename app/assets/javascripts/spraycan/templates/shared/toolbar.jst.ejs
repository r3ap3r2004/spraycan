<h3>
  <a href="#">Spraycan</a>
</h3>
<ul class="nav">
  <li id="theme" class="dropdown<%= Spraycan.current == 'themes' ? ' active' : '' %>">
    <a rel="theme" href="#theme?all">Themes</a>
  </li>
  <li id="html" class="dropdown<%= Spraycan.current == 'view_overrides' ? ' active' : '' %>" data-dropdown="dropdown">
    <a rel="html" href="#" class="dropdown-toggle">HTML</a>
    <ul class="dropdown-menu">
      <li>
        <a href="#view_override?new=new_override">New View Override</a>
      </li>
      <li>
        <a href="#view_override?all">Load View Overrides</a>
      </li>
      <li class="divider"></li>
      <li>
        <a href="#inspect">Show Inspector</a>
      </li>
      <% if(_.size(Spraycan.current_collections.view_overrides) > 0){ %>
        <li class="divider"></li>
      <% } %>
      <% _.each(Spraycan.current_collections.view_overrides.reverse(), function(model) { %> 
        <li>
          <a href="#view_override/<%= model.cid %>">
            <%= model.get('name') %><% if(model.id==undefined){ %> *<% } %>
          </a>
        </li>
      <% }); %>
    </ul>
  </li>

  <li id='css' class="dropdown<%= Spraycan.current == 'stylesheets' ? ' active' : '' %>" data-dropdown="dropdown">
    <a rel="css" href="#" class="dropdown-toggle">CSS</a>
    <ul class="dropdown-menu">
      <li>
        <a href="#stylesheet?new">New Stylesheet</a>
      </li>
      <li>
        <a href="#stylesheet?all">Load Stylesheet</a>
      </li>
      <% if(_.size(Spraycan.current_collections.stylesheets) > 0){ %>
        <li class="divider"></li>
      <% } %>
      <% _.each(Spraycan.current_collections.stylesheets.reverse(), function(model) { %> 
        <li>
          <a href="#stylesheet/<%= model.cid %>">
            <%= model.get('name') %><% if(model.id==undefined){ %> *<% } %>
          </a>
        </li>
      <% }); %>
    </ul>

  </li>

  <li id='js' class="dropdown<%= Spraycan.current == 'javascripts' ? ' active' : '' %>" data-dropdown="dropdown">
    <a rel="js" href="#" class="dropdown-toggle">JS</a>
    <ul class="dropdown-menu">
      <li>
        <a href="#javascript?new">New Javascript</a>
      </li>
      <li>
        <a href="#javascript?all">Load Javascript</a>
      </li>
      <% if(_.size(Spraycan.current_collections.javascripts) > 0){ %>
        <li class="divider"></li>
      <% } %>
      <% _.each(Spraycan.current_collections.javascripts.reverse(), function(model) { %> 
        <li>
          <a href="#javascript/<%= model.cid %>">
            <%= model.get('name') %><% if(model.id==undefined){ %> *<% } %>
          </a>
        </li>
      <% }); %>
    </ul>
  </li>

  <li id='files' class="<%= Spraycan.current == 'files' ? ' active' : '' %>" ">
    <a rel="files" href="#file?all">Files</a>
  </li>
</ul>

<ul class="nav secondary-nav">
  <li id='resize' class="dropdown pull-right" data-dropdown="dropdown">
    <a rel="view" href="#" class="dropdown-toggle">Resize</a>
    <ul class="dropdown-menu">
      <li>
        <a href="#minimize">Minimize</a>
      </li>
      <li id="resize_restore">
        <a href="#restore">Restore</a>
      </li>
      <li>
        <a href="#maximize">Maximize</a>
      </li>
    </ul>
  </li>
  <li id="restore">
    <a rel="restore" href="#restore">Restore</a>
  </li>
  <li>
    <img src="/assets/spraycan/busy.gif" id="busy"/>
  </li>
</ul>
